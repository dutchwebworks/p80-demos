---
import Layout from "../layouts/Layout.astro";

const title = "HTTP 404 Not found";
const description = "Multiple Poort80 Demos";
const permalink = `${Astro.site.href}`;
---

<Layout title={title} description={description} permalink={permalink} showBackButton={true}>
    <main>
        <h2>Error 404</h2>

        <p>Page not found. Please go back the <a href="/">homepage</a>.</p>
    </main>
</Layout>
